<p id="notice"><%= notice %></p>

<h1>Listing Payment Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @payment_users.each do |payment_user| %>
      <tr>
        <td><%= link_to 'Show', payment_user %></td>
        <td><%= link_to 'Edit', edit_payment_user_path(payment_user) %></td>
        <td><%= link_to 'Destroy', payment_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Payment user', new_payment_user_path %>
